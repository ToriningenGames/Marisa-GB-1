            <WordsStyle name="LINE COMMENTS" fgColor="009726" bgColor="FFFFFF" colorStyle="1" fontName="Courier New" fontStyle="0" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="80FFFF" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="4646FF" bgColor="FFFFFF" colorStyle="1" fontName="Courier New" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="FF8000" bgColor="FFFFFF" colorStyle="1" fontName="Courier New" fontStyle="0" fontSize="9" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="FFFF00" bgColor="FFFFFF" colorStyle="1" fontName="Courier New" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="60FF00" bgColor="FFFFFF" colorStyle="1" fontName="Courier New" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="FF8000" bgColor="FFFFFF" colorStyle="1" fontName="" fontStyle="0" nesting="0" />
